<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <display-name>WeatherApp</display-name>
    <servlet>
        <servlet-name>AppLauncher</servlet-name>
        <servlet-class>AppLauncher</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WeatherAppServlet</servlet-name>
        <servlet-class>WeatherAppServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet1</servlet-name>
        <servlet-class>NewServlet1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AppLauncher</servlet-name>
        <url-pattern>/AppLauncher</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>WeatherAppGui</servlet-name>
        <servlet-class>WeatherAppGui</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WeatherAppGui</servlet-name>
        <url-pattern>/WeatherAppGui</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WeatherAppServlet</servlet-name>
        <url-pattern>/WeatherAppServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet1</servlet-name>
        <url-pattern>/NewServlet1</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>AppLauncher</welcome-file>
    </welcome-file-list>
</web-app>
